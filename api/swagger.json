{
	"basePath": "/",
	"definitions": {},
	"info": {
		"description": "A Typescript framework for home automation",
		"title": "domoja",
		"version": "0.0.13"
	},
	"paths": {
		"/devices": {
			"get": {
				"operationId": "DevicesServiceGetDevices",
				"produces": [
					"text/html"
				],
				"responses": {
					"204": {
						"description": "No content"
					}
				},
				"description": "Retrieves the list of devices",
				"parameters": []
			}
		},
		"/devices/{id}": {
			"get": {
				"operationId": "DevicesServiceGet",
				"produces": [
					"text/html"
				],
				"responses": {
					"204": {
						"description": "No content"
					}
				},
				"description": "Retrieves a device",
				"parameters": [
					{
						"description": "path of the device",
						"in": "path",
						"name": "id",
						"required": true,
						"type": "string"
					}
				]
			},
			"post": {
				"operationId": "DevicesServiceSendCommand",
				"produces": [
					"text/html"
				],
				"responses": {
					"204": {
						"description": "No content"
					}
				},
				"description": "Sends a command to a device",
				"consumes": [
					"application/x-www-form-urlencoded"
				],
				"parameters": [
					{
						"description": "path of the device",
						"in": "path",
						"name": "id",
						"required": true,
						"type": "string"
					},
					{
						"description": "command sent to the device",
						"in": "formData",
						"name": "command",
						"required": true,
						"type": "string"
					}
				]
			}
		},
		"/pages": {
			"get": {
				"operationId": "PagesServiceGetPages",
