					"text/html"
				],
				"responses": {
					"204": {
						"description": "No content"
					}
				},
				"description": "Set the app demo mode",
				"consumes": [
					"application/x-www-form-urlencoded"
				],
				"parameters": [
					{
						"description": "",
						"in": "formData",
						"name": "value",
						"required": true,
						"type": "boolean"
					}
				]
			}
		}
